{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "paths": {
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
    },
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types", "src/types/*.d.ts"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  },
  "include": [
    "src/**/*"
  ]
}